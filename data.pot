[physics]
E = 25000                       % Young modulus
nu = 0.3                        % Poisson ratio
bulkLoad = [0.0,0.0,-9.81]    % gravity
[../]

[domain]
Lx = 4                          % Lenght of domain side along x
Ly = 8                          % Lenght of domain side along y
Lz = 8                          % Lenght of domain side along z
Nx = 16                          % Number of subdivisions along x
Ny = 2                          % Number of subdivisions along y
Nz = 2                          % Number of subdivisions along z
meshType = GT_QK(3,1)
[../]

[it]
ContactToll = 1.0e-5            % tolerance for contact
ContactMaxIt = 500              % maximum number of iteration
theta = 0                       % +1 0 -1 Nitsche 
gamma = 10                      % Nitsche parameter
[../]

[time]
t0 = 0.0                        % starting time
tend = 0.5                      % ending time
dt = 0.05                       % timestep
[../]

[numerics]
GT_PK(3,1)"
