[domain]
angle = 2                       % Inclination degree (ignored if the mesh is built-in)
Lx = 1                          % Lenght of domain side along x
Ly = 1                          % Lenght of domain side along y
Lz = 1                          % Lenght of domain side along z
h = 0.5                         % Grid spacing
meshType = tetra                % Element type: tetra hexa
order_u = 1                     % Order of Lagrangian Finite Elements for displacement
order_lm = 0                    % Order of Lagrangian Finite Elements for LM (used iff method==augLM)
[../]

[physics]
E = 25000                       % Young modulus
nu = 0.25                       % Poisson ratio
bulkLoad = [0.,0.,-0.]          % Gravity
mu_friction = 0.4               % Friction coefficient

regionDisp = [1,5]              % Dirichlet bd regions
bdDisp1 = {0.,0.,0.}
bdDisp2 = {0.,0.,0.}

regionDispNormal = [9]          % Normal Dirichlet bd regions
bdDispN1 = 0.

regionLoad = [7,10]             % Neumann bd regions
bdLoad1 = {0,0,-t}
bdLoad2 = {-3,0,0}
[../]

[it]
maxit = 50                      % Maximum number of iteration
atol = 1.0e-12                  % Residual tolerance
[../]

[contact]
method = nitsche                % nitsche penalty
theta = 1                       % +1 0 -1 Nitsche 
gammaN = 10                     % Nitsche parameter
gammaP = 1.0e5                  % Penalty parameter
gammaL = 1.0e2                  % augmented Lagrange parameter
[../]

[time]
t0 = 0.0                       % Starting time
tend = 10.5                       % Ending time
dt = 0.5                       % Timestep
[../]
